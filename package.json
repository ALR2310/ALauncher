  "scripts": {
    "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "cd src-client && vite --host",
    "dev:server": "cd src-server && tsx watch --clear-screen=false --inspect src/index.ts",
    "preview": "yarn build:client && concurrently \"vite preview\" \"yarn dev:server\"",
    "tauri": "tauri",
    "update": "npm-check-updates -u",
    "build": "tsx scripts/build/index.ts",
    "build:client": "cd src-client && vite build",
    "build:server": "tsx scripts/build/server.ts",
    "release": "tsx scripts/build/release.ts"
  },
