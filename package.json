{
  "name": "alauncher",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --host\" \"yarn dev:server\"",
    "dev:server": "tsx watch --clear-screen=false src-server/index.ts",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint",
    "update": "npm-check-updates -u",
    "build": "tsc && vite build",
    "build:app": "tsx scripts/build.ts",
    "build:server": "esbuild server/index.ts --bundle --platform=node --target=node18 --minify --outfile=dist/server.js && pkg dist/server.js --targets node18-win-x64 --output src-tauri/binaries/server-x86_64-pc-windows-msvc.exe"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.1",
    "@tanstack/react-query": "^5.84.1",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-fs": "~2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "~2",
    "@tauri-apps/plugin-shell": "~2",
    "@tauri-apps/plugin-updater": "~2",
    "@types/react-router": "^5.1.20",
    "dayjs": "^1.11.13",
    "hono": "^4.9.0",
    "lodash": "^4.17.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.0",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tauri-apps/cli": "^2.7.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.2.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@yao-pkg/pkg": "^6.6.0",
    "concurrently": "^9.2.0",
    "daisyui": "5.0.50",
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1"
  }
}
