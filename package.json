{
  "name": "alauncher",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --host\" \"yarn dev:server\"",
    "dev:server": "tsx watch --clear-screen=false src-server/index.ts",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint",
    "update": "npm-check-updates -u",
    "build": "tsc && vite build",
    "build:app": "tsx scripts/build.ts",
    "build:server": "esbuild src-server/index.ts --bundle --minify --external:electron --platform=node --target=node18 --format=cjs --outfile=dist/server.cjs && pkg dist/server.cjs --targets node18-win-x64 --output src-tauri/binaries/server-x86_64-pc-windows-msvc.exe"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.2",
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/plugin-fs": "~2.4.1",
    "@tauri-apps/plugin-opener": "^2.4.0",
    "@tauri-apps/plugin-process": "~2.3.0",
    "@tauri-apps/plugin-shell": "~2.3.0",
    "@tauri-apps/plugin-updater": "~2.9.0",
    "@types/react-router": "^5.1.20",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "prompt": "^1.3.0",
    "react-router": "^7.8.0",
    "react-router-dom": "^7.8.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tauri-apps/cli": "^2.7.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.2.1",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@yao-pkg/pkg": "^6.6.0",
    "concurrently": "^9.2.0",
    "daisyui": "5.0.50",
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
