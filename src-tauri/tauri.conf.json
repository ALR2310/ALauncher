{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ALauncher",
  "version": "1.0.0",
  "identifier": "com.alr.alauncher",
  "build": {
    "beforeDevCommand": "yarn build && yarn dev",
    "devUrl": "http://localhost:2310"
  },
  "app": {
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../dist/": ""
    }
  }
}
